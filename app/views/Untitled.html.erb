<%= form_tag(match_path(1), method: :get) do %>
  <div class="input-field">
    <%= label_tag :location %>
    <%= select_tag :location, options_from_collection_for_select(Organization.where(user_id: session[:user_id])[0].locations, :id, :branch_name) %>
  </div>

  <div class="input-field">
    <%= label_tag :survey %>
    <%= select_tag :survey, options_from_collection_for_select(Survey.all, :id, :name) %>
  </div>

  <div class="input-field">
    <%= label_tag :Type_of_Match %>
    <%= select_tag :match_type, options_for_select(["Resident -> Volunteer"], ["Volunteer -> Resident"]) %>
  </div>

  <div class="input-field">
    <%= label_tag :user %>
    <%= select_tag :user, options_from_collection_for_select(Resident.all, :id, :user_id) %>
  </div>
  <%= submit_tag %>
<% end %>
